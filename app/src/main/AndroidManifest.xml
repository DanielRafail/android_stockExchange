<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dawson.highwaytohell.fehighwaytohell">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ForeignExchange.ForeignExchangeActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".Settings"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".About"
            android:label="@string/app_name"></activity>
        <activity android:name=".Hints.HintActivity" />
        <activity android:name=".StockQuotes.StockQuotesActivity" />
        <activity android:name=".EditNotes" />
        <activity
            android:name=".LoanCalculator.CalculatorActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".LoanCalculator.ResultActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".Notes.ViewShortNotesFragment"
            android:label="NotesActivity" />
        <activity android:name=".Notes.ViewTotalNote" />
        <activity android:name=".Notes.CreateNote" />
        <activity android:name=".Portfolio.Portfolio"
            android:label="@string/app_name" />
        <activity android:name=".Portfolio.PortfolioAnswer"
            android:label="@string/app_name" />
        <activity android:name=".StockUp.StockBuyActivity" />
        <activity android:name=".StockDown.StockSellActivity" />

    </application>

</manifest>